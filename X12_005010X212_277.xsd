  <xs:complexType name="NM1_Type">
    <xs:sequence>
      <xs:element name="NM101" type="xs:string" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Entity Identifier Code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NM102" type="xs:string" minOccurs="0">
        <xs:annotation>
