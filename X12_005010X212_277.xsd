      </xs:element>
      <xs:element name="REF03" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Description</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

