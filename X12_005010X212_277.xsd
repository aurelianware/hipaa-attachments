  <xs:complexType name="HL_Type">
    <xs:sequence>
      <xs:element name="HL01" type="xs:string" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Hierarchical ID Number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="HL02" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Hierarchical Parent ID Number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="HL03" type="xs:string" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Hierarchical Level Code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="HL04" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Hierarchical Child Code</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

