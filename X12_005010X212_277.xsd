        </xs:annotation>
      </xs:element>
      <xs:element name="SE02" type="xs:string" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Transaction Set Control Number</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

</xs:schema>
