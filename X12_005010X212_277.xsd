        </xs:annotation>
      </xs:element>
      <xs:element name="NM108" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Identification Code Qualifier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NM109" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Identification Code</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- TRN - Trace -->
  <xs:complexType name="TRN_Type">
    <xs:sequence>
      <xs:element name="TRN01" type="xs:string" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Trace Type Code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TRN02" type="xs:string" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Reference Identification</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TRN03" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Originating Company Identifier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TRN04" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Reference Identification</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- STC - Status Information -->
  <xs:complexType name="STC_Type">
    <xs:sequence>
      <xs:element name="STC01_1" type="xs:string" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Health Care Claim Status Category Code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="STC01_2" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Status Code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="STC02" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Date</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="STC03" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Action Code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="STC10" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Status Information Effective Date</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- REF - Reference Information -->
  <xs:complexType name="REF_Type">
    <xs:sequence>
      <xs:element name="REF01" type="xs:string" minOccurs="1">
