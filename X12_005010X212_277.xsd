  <xs:complexType name="DTM_Type">
    <xs:sequence>
      <xs:element name="DTM01" type="xs:string" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Date/Time Qualifier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DTM02" type="xs:string" minOccurs="0">
